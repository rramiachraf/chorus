{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "include": ["src/**/*"],
  "exclude": ["node_modules/*", "__sapper__/*", "public/*"],
  "compilerOptions": {
	  "isolatedModules": true,
	  "useDefineForClassFields": true,
	  "types": ["vite/client"],
	  "sourceMap": true,
	  "importsNotUsedAsValues": "error"
  }
}
